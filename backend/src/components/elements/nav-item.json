{
  "collectionName": "components_elements_nav_items",
  "info": {
    "displayName": "Navigation Item",
    "description": "Single navigation menu item"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string"
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "isExternal": {
      "type": "boolean",
      "default": false
    },
    "openInNewTab": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
