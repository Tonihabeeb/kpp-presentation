<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f0f2f5"/>
  
  <!-- Manufacturing facility background -->
  <rect x="0" y="200" width="400" height="100" fill="#e1e5e9"/>
  
  <!-- Factory buildings -->
  <rect x="50" y="120" width="80" height="80" fill="#5f6368" rx="4"/>
  <rect x="150" y="100" width="100" height="100" fill="#34a853" rx="4"/>
  <rect x="270" y="130" width="80" height="70" fill="#4285f4" rx="4"/>
  
  <!-- Smokestacks (but clean) -->
  <rect x="80" y="80" width="8" height="40" fill="#9aa0a6"/>
  <rect x="100" y="70" width="8" height="50" fill="#9aa0a6"/>
  <rect x="200" y="60" width="8" height="40" fill="#9aa0a6"/>
  
  <!-- KPP systems -->
  <rect x="60" y="140" width="15" height="30" fill="#fbbc04" rx="2"/>
  <rect x="80" y="140" width="15" height="30" fill="#fbbc04" rx="2"/>
  <rect x="160" y="130" width="15" height="40" fill="#fbbc04" rx="2"/>
  <rect x="180" y="130" width="15" height="40" fill="#fbbc04" rx="2"/>
  <rect x="280" y="150" width="15" height="25" fill="#fbbc04" rx="2"/>
  <rect x="300" y="150" width="15" height="25" fill="#fbbc04" rx="2"/>
  
  <!-- Power lines -->
  <line x1="0" y1="50" x2="400" y2="50" stroke="#202124" stroke-width="2"/>
  <line x1="0" y1="55" x2="400" y2="55" stroke="#202124" stroke-width="2"/>
  
  <!-- Power transmission towers -->
  <polygon points="20,50 25,30 30,50" fill="#5f6368"/>
  <polygon points="120,50 125,25 130,50" fill="#5f6368"/>
  <polygon points="220,50 225,30 230,50" fill="#5f6368"/>
  <polygon points="320,50 325,25 330,50" fill="#5f6368"/>
  
  <!-- Title overlay -->
  <rect x="20" y="20" width="360" height="40" fill="rgba(26, 115, 232, 0.9)" rx="8"/>
  <text x="200" y="40" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="18" font-weight="bold">Manufacturing Facility</text>
  <text x="200" y="55" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12">KPP-Powered Production</text>
  
  <!-- Stats overlay -->
  <rect x="20" y="250" width="120" height="30" fill="rgba(52, 168, 83, 0.9)" rx="4"/>
  <text x="80" y="268" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">95% Efficiency</text>
  
  <rect x="260" y="250" width="120" height="30" fill="rgba(234, 67, 53, 0.9)" rx="4"/>
  <text x="320" y="268" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Zero Emissions</text>
</svg>
