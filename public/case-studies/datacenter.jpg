<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#0f1419"/>
  
  <!-- Data center building -->
  <rect x="60" y="80" width="280" height="160" fill="#1a1a1a" stroke="#34a853" stroke-width="2" rx="8"/>
  
  <!-- Server racks -->
  <rect x="80" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="100" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="120" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="140" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  
  <rect x="200" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="220" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="240" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="260" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="280" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  <rect x="300" y="100" width="15" height="80" fill="#4285f4" rx="2"/>
  
  <!-- LED indicators -->
  <circle cx="87.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="87.5" cy="120" r="2" fill="#34a853"/>
  <circle cx="87.5" cy="130" r="2" fill="#fbbc04"/>
  <circle cx="107.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="107.5" cy="120" r="2" fill="#34a853"/>
  <circle cx="127.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="147.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="147.5" cy="120" r="2" fill="#34a853"/>
  
  <circle cx="207.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="227.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="247.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="267.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="287.5" cy="110" r="2" fill="#34a853"/>
  <circle cx="307.5" cy="110" r="2" fill="#34a853"/>
  
  <!-- KPP power systems -->
  <rect x="20" y="200" width="30" height="50" fill="#34a853" rx="4"/>
  <rect x="350" y="200" width="30" height="50" fill="#34a853" rx="4"/>
  
  <!-- Power indicators -->
  <circle cx="35" cy="215" r="6" fill="#fbbc04"/>
  <text x="35" y="219" text-anchor="middle" fill="black" font-family="Arial, sans-serif" font-size="8" font-weight="bold">P</text>
  <circle cx="365" cy="215" r="6" fill="#fbbc04"/>
  <text x="365" y="219" text-anchor="middle" fill="black" font-family="Arial, sans-serif" font-size="8" font-weight="bold">P</text>
  
  <!-- Cooling systems -->
  <rect x="160" y="200" width="80" height="30" fill="#4285f4" rx="4"/>
  <text x="200" y="218" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10">COOLING</text>
  
  <!-- Network connections -->
  <line x1="87.5" y1="180" x2="200" y2="100" stroke="#fbbc04" stroke-width="1"/>
  <line x1="107.5" y1="180" x2="200" y2="100" stroke="#fbbc04" stroke-width="1"/>
  <line x1="227.5" y1="180" x2="200" y2="100" stroke="#fbbc04" stroke-width="1"/>
  <line x1="287.5" y1="180" x2="200" y2="100" stroke="#fbbc04" stroke-width="1"/>
  
  <!-- Title overlay -->
  <rect x="20" y="20" width="360" height="40" fill="rgba(26, 115, 232, 0.9)" rx="8"/>
  <text x="200" y="40" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="18" font-weight="bold">Data Center</text>
  <text x="200" y="55" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12">Mission-Critical Power</text>
  
  <!-- Stats -->
  <rect x="20" y="260" width="120" height="30" fill="rgba(52, 168, 83, 0.9)" rx="4"/>
  <text x="80" y="278" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">99.99% Uptime</text>
  
  <rect x="260" y="260" width="120" height="30" fill="rgba(234, 67, 53, 0.9)" rx="4"/>
  <text x="320" y="278" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Zero Downtime</text>
