<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#e8f0fe"/>
  
  <!-- Hospital building -->
  <rect x="100" y="80" width="200" height="150" fill="white" stroke="#e1e5e9" stroke-width="2" rx="8"/>
  
  <!-- Hospital cross -->
  <rect x="180" y="40" width="40" height="40" fill="#ea4335" rx="20"/>
  <rect x="195" y="50" width="10" height="20" fill="white"/>
  <rect x="190" y="55" width="20" height="10" fill="white"/>
  
  <!-- Windows -->
  <rect x="110" y="100" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="130" y="100" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="150" y="100" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="235" y="100" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="255" y="100" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="275" y="100" width="15" height="20" fill="#4285f4" rx="2"/>
  
  <rect x="110" y="140" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="130" y="140" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="150" y="140" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="235" y="140" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="255" y="140" width="15" height="20" fill="#4285f4" rx="2"/>
  <rect x="275" y="140" width="15" height="20" fill="#4285f4" rx="2"/>
  
  <!-- KPP units -->
  <rect x="50" y="180" width="20" height="40" fill="#34a853" rx="4"/>
  <rect x="330" y="180" width="20" height="40" fill="#34a853" rx="4"/>
  <circle cx="60" cy="200" r="5" fill="#fbbc04"/>
  <circle cx="340" cy="200" r="5" fill="#fbbc04"/>
  
  <!-- Entry -->
  <rect x="180" y="200" width="40" height="30" fill="#9aa0a6" rx="4"/>
  <text x="200" y="220" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10">MAIN</text>
  
  <!-- Ambulance -->
  <rect x="20" y="200" width="60" height="25" fill="white" stroke="#ea4335" stroke-width="2" rx="4"/>
  <circle cx="30" cy="235" r="8" fill="#202124"/>
  <circle cx="70" cy="235" r="8" fill="#202124"/>
  <rect x="25" y="205" width="8" height="8" fill="#ea4335"/>
  <rect x="27" y="207" width="4" height="4" fill="white"/>
  
  <!-- Title overlay -->
  <rect x="20" y="20" width="360" height="40" fill="rgba(234, 67, 53, 0.9)" rx="8"/>
  <text x="200" y="40" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="18" font-weight="bold">General Hospital</text>
  <text x="200" y="55" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12">Critical Power Reliability</text>
  
  <!-- Stats -->
  <rect x="20" y="250" width="120" height="30" fill="rgba(52, 168, 83, 0.9)" rx="4"/>
  <text x="80" y="268" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">99.9% Uptime</text>
  
  <rect x="260" y="250" width="120" height="30" fill="rgba(26, 115, 232, 0.9)" rx="4"/>
  <text x="320" y="268" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="12" font-weight="bold">24/7 Power</text>
</svg>
