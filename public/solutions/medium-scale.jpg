<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#f0f2f5"/>
  
  <!-- Medium scale KPP system -->
  <rect x="120" y="80" width="160" height="140" fill="#34a853" stroke="#137333" stroke-width="3" rx="8"/>
  
  <!-- Main units -->
  <rect x="140" y="100" width="50" height="100" fill="white" stroke="#e1e5e9" stroke-width="1" rx="4"/>
  <rect x="210" y="100" width="50" height="100" fill="white" stroke="#e1e5e9" stroke-width="1" rx="4"/>
  
  <!-- Generators -->
  <circle cx="165" cy="150" r="20" fill="#4285f4"/>
  <text x="165" y="155" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10" font-weight="bold">GEN1</text>
  <circle cx="235" cy="150" r="20" fill="#4285f4"/>
  <text x="235" y="155" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="10" font-weight="bold">GEN2</text>
  
  <!-- Power output indicator -->
  <rect x="130" y="90" width="140" height="8" fill="#fbbc04" rx="4"/>
  <text x="200" y="87" text-anchor="middle" fill="#202124" font-family="Arial, sans-serif" font-size="12" font-weight="bold">1MW System</text>
  
  <!-- Base platform -->
  <rect x="110" y="220" width="180" height="25" fill="#9aa0a6" rx="4"/>
  
  <!-- Connection points -->
  <circle cx="140" cy="232" r="5" fill="#ea4335"/>
  <circle cx="165" cy="232" r="5" fill="#34a853"/>
  <circle cx="200" cy="232" r="5" fill="#4285f4"/>
  <circle cx="235" cy="232" r="5" fill="#fbbc04"/>
  <circle cx="260" cy="232" r="5" fill="#ea4335"/>
  
  <!-- Commercial building -->
  <rect x="30" y="160" width="60" height="70" fill="#f8f9fa" stroke="#dadce0" stroke-width="1" rx="4"/>
  <rect x="40" y="180" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="52" y="180" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="64" y="180" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="76" y="180" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="40" y="200" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="52" y="200" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="64" y="200" width="8" height="12" fill="#4285f4" rx="1"/>
  <rect x="76" y="200" width="8" height="12" fill="#4285f4" rx="1"/>
  <text x="60" y="175" text-anchor="middle" fill="#5f6368" font-family="Arial, sans-serif" font-size="9">OFFICE</text>
  
  <!-- Factory -->
  <rect x="310" y="170" width="70" height="60" fill="#f1f3f4" stroke="#5f6368" stroke-width="1" rx="4"/>
  <rect x="320" y="180" width="15" height="20" fill="#ea4335" rx="2"/>
  <rect x="340" y="180" width="15" height="20" fill="#ea4335" rx="2"/>
  <rect x="360" y="180" width="15" height="20" fill="#ea4335" rx="2"/>
  <text x="345" y="175" text-anchor="middle" fill="#5f6368" font-family="Arial, sans-serif" font-size="9">FACTORY</text>
  
  <!-- Connection lines -->
  <line x1="90" y1="195" x2="120" y2="160" stroke="#34a853" stroke-width="3"/>
  <line x1="280" y1="160" x2="310" y2="195" stroke="#34a853" stroke-width="3"/>
  
  <!-- Power flow indicators -->
  <polygon points="105,175 115,172 115,178" fill="#34a853"/>
  <polygon points="295,175 285,172 285,178" fill="#34a853"/>
  
  <!-- Title overlay -->
  <rect x="20" y="20" width="360" height="50" fill="rgba(52, 168, 83, 0.9)" rx="8"/>
  <text x="200" y="40" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="20" font-weight="bold">Medium Scale KPP</text>
  <text x="200" y="58" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="14">Commercial & Industrial</text>
</svg>
